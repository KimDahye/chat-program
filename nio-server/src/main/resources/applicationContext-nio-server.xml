<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <context:annotation-config/>
    <bean id="RoomListManagerBean" class="RoomListManager" name = "roomListManager"/>

    <bean id="ChatEventHandler" class="ChatEventHandler" scope="prototype"/>
    <bean id="FileEventHandler" class="FileEventHandler" scope="prototype"/>
    <bean id="RoomNameEventHandler" class="RoomNameEventHandler" scope="prototype"/>
    <bean id="RoomNumberEventHandler" class="RoomNumberEventHandler" scope="prototype"/>
    <bean id="UserNameEventHandler" class="UserNameEventHandler" scope="prototype"/>
</beans>